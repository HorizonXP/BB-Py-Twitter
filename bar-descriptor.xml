<?xml version="1.0" encoding="utf-8" standalone="no"?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>org.BB-Py.samples.twitter</id>
    <name>BB-Py Twitter</name>
    <versionNumber>0.0.1</versionNumber>
    <buildIdFile>0</buildIdFile>
    <description>Sample Twitter client app using BB-Py</description>
    <initialWindow>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
    </initialWindow>
    <icon><image>assets/icon.png</image></icon>
    <asset path="assets/icon.png">icon.png</asset>
    <asset path="main.py" entry="true" type="Qnx/Python">main.py</asset>
    <action system="true">run_native</action>
    <action>access_shared</action>
    <arg>-shared</arg>
</qnx>
